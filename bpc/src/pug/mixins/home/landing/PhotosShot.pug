mixin PhotoItems(config)
	.col-sm-6.col-md-4.mb-3.h-100
		.card.card-span.h-100.text-white
			img.rounded-3(src=`${CWD}assets/img/gallery/${config.image}` alt='...').h-100
			a.stretched-link(href='#!')

mixin PhotosShot
	section
		.container
			.row.h-100
				.col-lg-7.mx-auto.text-center.mb-6
					h1.text-center.lh-sm.fs-lg-6.fs-xxl-7 Over 1,00,000 Photos Shot

				.col-12
					nav
					.nav.nav-tabs.clickr-tabs.mb-6.justify-content-center#nav-tab(role='tablist')
						button.nav-link.active#nav-food-tab(data-bs-toggle='tab' data-bs-target='#nav-food' type='button' role='tab' aria-controls='nav-food' aria-selected='true') Food
						button.nav-link#nav-shoes-tab(data-bs-toggle='tab' data-bs-target='#nav-shoes' type='button' role='tab' aria-controls='nav-shoes' aria-selected='false') Shoes
						button.nav-link#nav-watches-tab(data-bs-toggle='tab' data-bs-target='#nav-watches' type='button' role='tab' aria-controls='nav-watches' aria-selected='false') Watches
						button.nav-link#nav-cosmetics-tab(data-bs-toggle='tab' data-bs-target='#nav-cosmetics' type='button' role='tab' aria-controls='nav-cosmetics' aria-selected='false') Cosmetics
						button.nav-link#nav-phone-tab(data-bs-toggle='tab' data-bs-target='#nav-phone' type='button' role='tab' aria-controls='nav-phone' aria-selected='false') Phone
						button.nav-link#nav-camera-tab(data-bs-toggle='tab' data-bs-target='#nav-camera' type='button' role='tab' aria-controls='nav-camera' aria-selected='false') Camera
				.tab-content#nav-tabContent
					.tab-pane.fade.show.active#nav-food(role='tabpanel' aria-labelledby='nav-food-tab') 
						.row.gx-3
							+PhotoItems(category[0])
							+PhotoItems(category[1])
							+PhotoItems(category[2])
							+PhotoItems(category[3])
							+PhotoItems(category[4])
							+PhotoItems(category[5])
							.col-12.d-flex.justify-content-center.mt-5                                            
								a.btn.btn-lg.btn-outline-warning.rounded-pill(href='#!') View portfolio
									i.fas.fa-arrow-right.ms-2
					.tab-pane.fade#nav-shoes(role='tabpanel' aria-labelledby='nav-shoes-tab') 
						.row.gx-3
							+PhotoItems(category[6])
