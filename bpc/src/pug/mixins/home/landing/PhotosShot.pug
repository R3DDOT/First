								a.btn.btn-lg.btn-outline-warning.rounded-pill(href='#!') View portfolio
									i.fas.fa-arrow-right.ms-2
					.tab-pane.fade#nav-watches(role='tabpanel' aria-labelledby='nav-watches-tab')
